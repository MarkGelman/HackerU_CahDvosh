<Window x:Class="MultiPage_AllPageInList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiPage_AllPageInList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Background="White" Orientation="Horizontal">
            <Button x:Name="PrevPage" Content="&lt;&lt;" Height="35" MinWidth="100" Margin="5,5,5,5" Click="PrevPage_Click"></Button>
            <Button x:Name="NextPage" Content="&gt;&gt;" Height="35" MinWidth="100" Margin="5,5,5,5" Click="NextPage_Click"></Button>
        </StackPanel>
        <Frame x:Name="MyFrame" Grid.Row="1" Background="AliceBlue" />
    </Grid>

   
</Window>
